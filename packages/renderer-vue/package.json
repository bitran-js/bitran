{
    "name": "@bitran-js/renderer-vue",
    "version": "1.0.0-dev.9",
    "type": "module",
    "description": "Bitran Vue Renderer",
    "keywords": [
        "bitran",
        "text",
        "vue"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Gwynerva/bitran.git",
        "directory": "packages/renderer-vue"
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.mjs"
        },
        "./scss/*": "./dist/public/scss/*"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "bun run dev.ts",
        "build": "bun mkdist -d --loaders=js",
        "prepack": "bun run build"
    },
    "peerDependencies": {
        "@bitran-js/core": "workspace:1.0.0-dev.9",
        "@bitran-js/transpiler": "workspace:1.0.0-dev.9",
        "vue": "3.5.x",
        "sass": "1.85.x"
    },
    "devDependencies": {
        "vite": "^6.2.1",
        "vue-tsc": "^2.2.8",
        "mkdist": "^2.2.0"
    }
}
